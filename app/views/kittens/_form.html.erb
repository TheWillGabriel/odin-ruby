<%= form_with model: @kitten, local: true do |f| %>
  <%= f.text_field :name, placeholder: 'Name' %>
  <%= f.number_field :age, placeholder: 'Age' %>
  <%= f.number_field :cuteness, in: 1..11, placeholder: 'Cuteness (1-11)' %>
  <%= f.number_field :softness, in: 1..11, placeholder: 'Softness (1-11)' %>
  <%= f.submit "Submit" %>
<% end %>